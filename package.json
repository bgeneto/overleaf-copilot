{
  "name": "ai-agent-for-overleaf",
  "version": "0.1.19",
  "description": "AI-powered assistant for Overleaf LaTeX editor",
  "private": true,
  "scripts": {
    "watch": "webpack --mode=development --watch --config config/webpack.config.js",
    "build": "webpack --mode=production --config config/webpack.config.js",
    "pack": "node pack.js --chrome",
    "pack:chrome": "node pack.js --chrome",
    "pack:firefox": "node pack.js --firefox",
    "pack:all": "node pack.js --chrome && node pack.js --firefox",
    "repack": "npm run build && npm run pack:all",
    "repack:chrome": "npm run build && npm run pack:chrome",
    "repack:firefox": "npm run build && npm run pack:firefox",
    "format": "prettier --write --ignore-unknown \"{config,public,src}/**/*.{html,css,js,ts,json}\""
  },
  "devDependencies": {
    "@types/chrome": "^0.0.181",
    "@types/dompurify": "^3.2.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "adm-zip": "^0.5.10",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^2.8.8",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@types/crypto-js": "^4.1.2",
    "@types/diff": "^5.2.3",
    "@types/marked": "^6.0.0",
    "@types/node": "^20.6.2",
    "crypto-js": "^4.1.1",
    "diff": "^7.0.0",
    "dompurify": "^3.3.1",
    "lucide": "^0.453.0",
    "lucide-preact": "^0.453.0",
    "marked": "^17.0.1",
    "preact": "^10.24.3",
    "purecss": "^3.0.0",
    "rss-parser": "^3.13.0"
  }
}